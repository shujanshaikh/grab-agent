{
  "name": "grab-agent",
  "type": "module",
  "version": "0.0.2",
  "prepare": "bun run build",
  "bin": {
    "grab-it": "./dist/cli.js",
    "grab-agent": "./dist/cli.js"
  },
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./dist/*": "./dist/*.js",
    "./dist/*.js": "./dist/*.js"
  },
  "browser": "dist/client.global.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -rf dist && NODE_ENV=production tsup"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.44",
    "@hono/node-server": "^1.19.7",
    "ai": "^5.0.108",
    "hono": "^4.10.7",
    "picocolors": "^1.1.1",
    "react-grab": "^0.0.74",
    "tsup": "^8.5.1"
  }
}
